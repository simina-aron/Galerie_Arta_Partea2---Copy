<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Galerie_Arta_Partea2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Galerie_Arta_Partea2"
    Shell.FlyoutBehavior="Disabled"
    Title="Galerie_Arta_Partea2">

    <TabBar>
        <ShellContent
            Title="Acasă"  
            ContentTemplate="{DataTemplate local:MainPage}"
         />
        <ShellContent  
            Title="Despre"
            ContentTemplate="{DataTemplate local:Despre}" 
         />
        <ShellContent
            Title="Tablouri"
            ContentTemplate="{DataTemplate local:Tablouri}"
         />
        <ShellContent
            Title="Compoziție și îngrijire"
            ContentTemplate="{DataTemplate local:Compoziție_și_îngrijire}"
         />
        <ShellContent
            Title="Programările mele"
            ContentTemplate="{DataTemplate local:ProgramareEntryPage}"
         />  
    </TabBar>

</Shell>
